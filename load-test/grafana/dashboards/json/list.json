          "pluginVersion": "4.11.2",
          "queryType": "table",
          "rawSql": "SELECT\n    t.test_name,\n    t.test_id,\n    t.url,\n    count() AS request_count,\n    min(r.timestamp) AS start_time,\n    max(r.timestamp) - min(r.timestamp) AS test_duration,\n    countIf(r.status = 'success') / count() as success_ratio\nFROM load_test.load_tests AS t\nLEFT JOIN load_test.requests AS r\n    ON t.test_id = r.test_id\nGROUP BY\n    t.test_name,\n    t.test_id,\n    t.url\nORDER BY\n    start_time ASC",
